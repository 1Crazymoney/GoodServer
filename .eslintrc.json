{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "semi":"off",
    "quotes":["error","double"],
    "require-jsdoc":"warn",
    "no-unused-vars":"warn"
  }
}
